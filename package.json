{
  "name": "next-warp-core",
  "description": "customized next-js environment for web3 development",
  "version": "0.0.3",
  "private": true,
  "workspaces": [
    "app",
    "chain"
  ],
  "scripts": {
    "chain": "yarn workspace @next-warp-core/chain chain",
    "start": "yarn workspace @next-warp-core/app start",
    "dev": "yarn workspace @next-warp-core/app dev",
    "build": "yarn workspace @next-warp-core/app build",
    "test:app": "yarn workspace @next-warp-core/app test",
    "eject": "yarn workspace @next-warp-core/app eject",
    "compile": "yarn workspace @next-warp-core/chain compile",
    "test:chain": "yarn workspace @next-warp-core/chain test",
    "deploy": "yarn workspace @next-warp-core/chain deploy"
  },
  "devDependencies": {
    "ts-node": "^9.1.1"
  }
}
